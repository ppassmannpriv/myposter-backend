    "doctrine/orm": "^2.14",
    "doctrine/dbal": "^3.5",
    "ext-pdo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "vimeo/psalm": "^5.4",
    "dg/bypass-finals": "^1.4"
  }
}
