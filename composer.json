  "description": "Myposter Backend Application Project",
  "type": "project",
  "autoload": {
    "psr-4": {
      "Myposter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Myposter\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.4 || ^8.2",
